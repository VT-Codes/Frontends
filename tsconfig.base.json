{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": "./",
    "rootDir": ".",
    "allowUnreachableCode": false,
    "forceConsistentCasingInFileNames": false,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "pretty": true,
    "removeComments": true,
    "newLine": "lf",
    "paths": {
      "@v-thomas/about/feature-home": ["libs/about/feature-home/src/index.ts"],
      "@v-thomas/about/feature-projects": ["libs/about/feature-projects/src/public_api.ts"],
      "@v-thomas/about/shell": ["libs/about/shell/src/public_api.tsx"],
      "@v-thomas/about/theme": ["./libs/about/theme/src/public_api.ts"],
      "@v-thomas/about/ui": ["libs/about/ui/src/public_api.ts"],
      "@v-thomas/honey/app": ["./libs/honey/app/src/public_api.tsx"],
      "@v-thomas/honey/data-access": ["./libs/honey/data-access/src/public_api.ts"],
      "@v-thomas/media/*": ["./media/*"],
      "@v-thomas/shared/assets/*": ["./libs/shared/assets/src/*"],
      "@v-thomas/shared/hooks": ["libs/shared/hooks/src/public_api.ts"],
      "@v-thomas/shared/styles": ["./libs/shared/styles/globals.css"],
      "@v-thomas/shared/theme": ["./libs/shared/theme/src/public_api.ts"],
      "@v-thomas/shared/ui": ["./libs/shared/ui/src/public_api.ts"],
      "@v-thomas/shared/web-utils": ["./libs/shared/web-utils/src/public_api.ts"],
      "@v-thomas/thunder/assets/*": ["./libs/thunder/assets/src/*"],
      "@v-thomas/thunder/data-access": ["./libs/thunder/data-access/src/public_api.ts"],
      "@v-thomas/thunder/feature-groups": ["./libs/thunder/feature-groups/src/public_api.tsx"],
      "@v-thomas/thunder/feature-home": ["./libs/thunder/feature-home/src/public_api.tsx"],
      "@v-thomas/thunder/feature-invite": ["./libs/thunder/feature-invite/src/public_api.tsx"],
      "@v-thomas/thunder/hooks": ["./libs/thunder/hooks/src/public_api.ts"],
      "@v-thomas/thunder/shell": ["./libs/thunder/shell/src/public_api.tsx"],
      "@v-thomas/thunder/theme": ["./libs/thunder/theme/src/public_api.ts"],
      "@v-thomas/thunder/types": ["./libs/thunder/types/src/index.ts"],
      "@v-thomas/thunder/ui": ["./libs/thunder/ui/src/public_api.ts"]
    }
  }
}
