{
  "root": "e2e/core-ui-e2e",
  "sourceRoot": "e2e/core-ui-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "e2e/core-ui-e2e/cypress.json",
        "devServerTarget": "core-ui:storybook"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "core-ui:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["e2e/core-ui-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["core-ui"]
}
