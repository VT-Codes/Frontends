{
  "root": "packages/structer",
  "sourceRoot": "packages/structer",
  "projectType": "library",
  "targets": {
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "packages/structer",
        "command": "yarn"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "packages/structer/jest.config.js"
      }
    },
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "entryFile": "packages/structer/src/public_api.ts",
        "tsConfig": "packages/structer/tsconfig.json",
        "outputPath": "dist/packages/structer",
        "optimization": true,
        "outputFileName": "main.js",
        "sourceMap": false,
        "project": "packages/structer/package.json",
        "format": ["esm", "umd"],
        "compiler": "swc",
        "assets": [
          {
            "glob": "packages/structer/*.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "sh ./scripts/publish.sh structer",
        "outputPath": "dist/packages/structer"
      }
    }
  }
}
