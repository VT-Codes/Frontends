{
  "root": "libs/thunder/shell",
  "sourceRoot": "libs/thunder/shell/src",
  "projectType": "library",
  "tags": ["scope:thunder", "type:shell"],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ],
      "options": {
        "outputPath": "dist/libs/thunder/shell",
        "tsConfig": "libs/thunder/shell/tsconfig.lib.json",
        "project": "libs/thunder/shell/package.json",
        "entryFile": "libs/thunder/shell/src/public_api.tsx",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "swc",
        "extractCss": true,
        "assets": [
          {
            "glob": "libs/thunder/shell/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/thunder/shell/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/thunder/shell"],
      "options": {
        "jestConfig": "libs/thunder/shell/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
