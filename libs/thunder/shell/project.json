{
  "root": "libs/thunder/shell",
  "sourceRoot": "libs/thunder/shell",
  "projectType": "library",
  "implicitDependencies": [
    "thunder-feature-groups",
    "thunder-feature-invite",
    "thunder-feature-home",
    "thunder-feature-404-page",
    "!thunder-ui"
  ],
  "tags": ["scope:thunder", "type:feature"],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/thunder/shell",
        "tsConfig": "libs/thunder/shell/tsconfig.lib.json",
        "project": "libs/thunder/shell/package.json",
        "entryFile": "libs/thunder/shell/src/public_api.tsx",
        "external": ["react/jsx-runtime", "@v-thomas/shared/ui", "@v-thomas/thunder/data-access"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "swc",
        "updateBuildableProjectDepsInPackageJson": true,
        "deleteOutputPath": true,
        "assets": [
          {
            "glob": "libs/thunder/shell/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "globals": [
          {
            "moduleId": "@v-thomas/thunder/data-access",
            "global": "@v-thomas/thunder/data-access"
          },
          {
            "moduleId": "@v-thomas/shared/core-ui",
            "global": "@v-thomas/shared/core-ui"
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/thunder/shell/**/*.{ts,tsx,js,jsx}",
          "libs/thunder/shell/**/*.spec.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/thunder/shell"],
      "options": {
        "jestConfig": "libs/thunder/shell/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "commitMessageFormat": "chore(${projectName}): release version ${version}"
      }
    }
  }
}
