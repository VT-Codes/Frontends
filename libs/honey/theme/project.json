{
  "root": "libs/honey/theme",
  "sourceRoot": "libs/honey/theme",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/honey/theme",
        "entryFile": "libs/honey/theme/src/public_api.ts",
        "tsConfig": "libs/honey/theme/tsconfig.lib.json",
        "assets": ["libs/honey/theme/README.md"],
        "project": "libs/honey/theme/package.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/honey/theme/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/honey/theme"],
      "options": {
        "jestConfig": "libs/honey/theme/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:honey type:other"]
}
