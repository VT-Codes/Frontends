{
  "name": "@v-thomas/thunderbolt",
  "version": "0.0.1",
  "description": "This is the backend for thunder",
  "author": "Vincent Thomas",
  "private": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/VincentThomas06/Codebase.git",
    "directory": "apps/thunderbolt",
    "type": "git"
  },
  "engines": {
    "node": "14 || 16"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "prebuild": "rimraf ./dist && npx prisma db push",
    "postbuild": "rimraf ./dist/tsconfig.build.tsbuildinfo",
    "build": "nest build --webpack --webpackPath webpack.config.js",
    "start:prod": "node dist/main",
    "start:dev": "nest start --watch",
    "start:webpack": "npm run prestart && npx nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "npx nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma": "prisma"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.6",
    "@nestjs/common": "^8.4.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-fastify": "^8.4.3",
    "@prisma/client": "^3.9.2",
    "@vendia/serverless-express": "^4.5.4",
    "axios": "^0.26.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.3",
    "fastify-static": "^4.6.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@swc/core": "^1.2.160",
    "@types/aws-lambda": "^8.10.93",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prettier-linter-helpers": "^1.0.0",
    "prisma": "^3.9.2",
    "run-script-webpack-plugin": "^0.0.11",
    "serverless-offline": "^8.5.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.1",
    "serverless-webpack": "^5.6.1",
    "serverless-webpack-prisma": "^1.1.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "swc-loader": "^0.1.15",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.2",
    "webpack": "5.71.0",
    "webpack-node-externals": "^3.0.0"
  }
}
