{
  "root": "apps/about",
  "sourceRoot": "apps/about/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx-plugin-vite:build",
      "options": {
        "outDir": "dist",
        "configFile": "apps/about/vite.config.ts",
        "watch": false,
        "write": true,
        "emitAtRootLevel": false,
        "manifest": true
      }
    },
    "serve": {
      "executor": "nx-plugin-vite:serve",
      "options": {
        "configFile": "apps/about/vite.config.ts",
        "port": 3000,
        "root": "apps/about",
        "host": true,
        "https": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/about/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/about"],
      "options": {
        "jestConfig": "apps/about/jest.config.js",
        "passWithNoTests": true
      }
    },
    "preview": {
      "executor": "nx-plugin-vite:preview",
      "options": {
        "configFile": "apps/about/vite.config.ts"
      }
    }
  },
  "tags": []
}
